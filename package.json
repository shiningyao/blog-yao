{
  "name": "blog-yao",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --public-host localhost:4200",
    "start:locale": "ng serve --host 0.0.0.0 --public-host localhost:4200 --configuration=locale-$BUILD_LOCALE",
    "start:zh": "BUILD_LOCALE=zh npm run start:locale",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prerender": "ts-node prerender.ts",
    "blog-yao:build": "ng run blog-yao:build --target=production --configuration=$configuration",
    "server": "configuration=production npm run blog-yao:build && ng run blog-yao:server && nodemon server.ts",
    "server:zh": "configuration=locale-zh-prod npm run blog-yao:build && ng run blog-yao:server --configuration=locale-zh && locale=zh nodemon server.ts",
    "i18n": "ng xi18n --output-path locale"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.2",
    "@angular/common": "^6.1.2",
    "@angular/compiler": "^6.1.2",
    "@angular/core": "^6.1.2",
    "@angular/forms": "^6.1.2",
    "@angular/http": "^6.1.2",
    "@angular/platform-browser": "^6.1.2",
    "@angular/platform-browser-dynamic": "^6.1.2",
    "@angular/platform-server": "^6.1.2",
    "@angular/router": "^6.1.2",
    "@nguniversal/common": "^6.0.0",
    "@nguniversal/express-engine": "^6.0.0",
    "@nguniversal/module-map-ngfactory-loader": "^6.0.0",
    "apollo-angular": "^1.1.2",
    "apollo-angular-link-http": "^1.1.1",
    "apollo-cache-inmemory": "^1.3.0-beta.6",
    "apollo-client": "^2.3.7",
    "apollo-link": "^1.2.2",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "moment": "^2.22.2",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.2.1",
    "tiny-slider-yao": "^2.6.0",
    "truncate-html": "^1.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.5",
    "@angular/cli": "~6.1.2",
    "@angular/compiler-cli": "^6.1.2",
    "@angular/language-service": "^6.1.2",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^10.5.7",
    "codelyzer": "~4.2.1",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "express-session": "^1.15.6",
    "hoek": "^4.2.1",
    "http-proxy-middleware": "^0.18.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^2.0.5",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5",
    "protractor": "~5.3.0",
    "ts-node": "^7.0.0",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "winston": "^3.0.0"
  }
}
