{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app-server",
    "baseUrl": ".",
    "module": "commonjs",
    "paths": {
      "@/*": ["app/*"],
      "#/*": ["assets/*"],
      "stream": ["../node_modules/readable-stream/readable.js"]
